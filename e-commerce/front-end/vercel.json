{
    "rewrites":[
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "builds":[
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "disDir": "build"
            }
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                { "key": "Cache-Control", "value": "no-store" }
            ]
        }
    ]
}
